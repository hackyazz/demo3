{
  "automock": false,
  "moduleFileExtensions": [
    "js"
  ],
  "testRegex": "lib/es6_global/test/.+test\\.js$",
  "transformIgnorePatterns": [
    "<rootDir>/node_modules/(?!(.*bs-)|reason-react|immutable-re|wonder-commonlib|wonder.js|wonder-log|wonder-most-animation-frame|wonder-webgl|wonder-imgui)"
  ],
  "snapshotResolver": "<rootDir>/customSnapshotResolver.js",
  "setupFilesAfterEnv": [
    "<rootDir>/test/helper/jest/specHelper.js"
  ],
  "notify": false,
  "verbose": false
}